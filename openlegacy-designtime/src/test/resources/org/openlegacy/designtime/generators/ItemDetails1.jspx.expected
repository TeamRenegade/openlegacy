<div xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:ol="urn:jsptagdir:/WEB-INF/tags">

	<form id="ItemDetails1Form" method="post" action="${pageContext.request.contextPath}/ItemDetails1">
		<div style="margin-left: 23%; top: 20%; width: 30%;" class="panel">
			<div class="field">
	<label>Item Number: </label>
		<span>${itemDetails1.itemNumber}</span>&amp;nbsp;
			</div>
			<div class="field">
	<label>Item Description: </label>
		<input type="text" name="itemDescription" value="${itemDetails1.itemDescription}" size="8" maxlength="8" />
			</div>
		</div>
		<div style="margin-left: 11%; top: 79%; width: 42%;position:absolute;" >
			<div class="field">
			<script>dojo.require("dijit.form.CheckBox");</script>
			<label> Pallet Label Required</label>
			<span</span>
			<input type="checkbox" data-dojo-type="dijit.form.CheckBox" name="palletLabelRequired" value="${itemDetails1.palletLabelRequired}" />
			</div>
		</div>
		
	</form>
</div>
