<div xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:ol="urn:jsptagdir:/WEB-INF/tags">
	<form id="ItemsListForm" method="post" action="${pageContext.request.contextPath}/ItemsList">
		<div style="margin-left: 6%; top: 83%; width: 30%;position:absolute;" >
			<div class="field">
	<label>Position To: </label>
		<span>${itemsList.positionTo}</span>&amp;nbsp;
			</div>
		</div>
		<div style="margin-left: 1%; top: 95%; width: 13%;position:absolute;" >
			<div class="field">
	<label>Error Message: </label>
		<span>${itemsList.errorMessage}</span>&amp;nbsp;
			</div>
		</div>
		<div style="margin-left: 5%; top: 25%; width: 80%;" class="panel">
				<table>
					<th>
					Action
					</th>
					<th>
					Alpha Search
					</th>
					<th>
					Item Description
					</th>
					<th>
					Item Number
					</th>
					<c:forEach items="${itemsList.itemsListRows}" var="item">
					<tr>
						<td>
							<select id="action" name="action" onchange="doPost()">
								<option value="">-Select-</option>
								<option value="1">View</option>
								<option value="2">Revise</option>
							</select>
						</td>
						<td>${item.alphaSearch}</td>
						<td>${item.itemDescription}</td>
						<td>${item.itemNumber}</td>
					</tr>
					</c:forEach>
				</table>
				<div class="excel">
					<a href="ItemsList/excel"><img src="images/excel.jpg" /></a>
				</div>
				<div class="paging">
					<a href="javascript:doAjaxPost('ItemsListForm','container','previous','body')">&lt; Previous</a>
					| 
					<a href="javascript:doAjaxPost('ItemsListForm','container','next','body')">Next &gt;</a> 
				</div>
		</div>
		
	</form>
</div>
