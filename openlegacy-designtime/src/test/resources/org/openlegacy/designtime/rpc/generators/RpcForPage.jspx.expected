<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:ol="urn:jsptagdir:/WEB-INF/tags/web" 
	xmlns:c="http://java.sun.com/jsp/jstl/core" 
	xmlns:fn="http://java.sun.com/jsp/jstl/functions"
	xmlns:spring="http://www.springframework.org/tags"
	xmlns:form="http://www.springframework.org/tags/form"
	version="2.0">
	<jsp:directive.page contentType="text/html;charset=UTF-8" pageEncoding="UTF-8" /> 
	<jsp:output omit-xml-declaration="yes" />
	
	<ol:form model="xxolakkrParms">
		<ol:pagePart title="LkCustomer">
	<ol:field name="lkCustomer.lkCustInd" />
	<ol:field name="lkCustomer.lkCustId" />
	<ol:field name="lkCustomer.lkCustName" />
	<ol:field name="lkCustomer.lkCustAddress" />
	<ol:field name="lkCustomer.lkCustPhone" />
		</ol:pagePart>
				<ol:table data="${xxolakkrParms.lkCustomer.lkCards}" property="lkCustomer.lkCards" actionsAsIcons="true"/>
	</ol:form>
</jsp:root>
