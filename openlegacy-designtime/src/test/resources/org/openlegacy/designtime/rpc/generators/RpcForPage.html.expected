<div class="row">
	<div class="col-sm-4">
		<h1>Xxolakkr Parms</h1>
	</div>
	<div class="col-sm-8">
		<h1>
			<button data-ng-click="update()" class="btn btn-lg btn-primary">Update</button>
		</h1>
	</div>
</div>

<form  role="form" accept-charset="utf-8">
	<div class="row">
		<div class="panel panel-default">
			<div class="panel-heading">Lk Customer</div>
			<div class="panel-body">
				<div class="row">
				<div class="col-sm-4">
					<div class="form-group">
					    <label for="lkCustomer.lkCustInd" class="col-sm-3 control-label">Lk Cust Ind:</label>
					    <div class="col-sm-9">
				   		<input type="text" class="form-control" id="lkCustomer.lkCustInd" ng-model="model.lkCustomer.lkCustInd" />
					   	</div>
					</div>
				</div>
				</div>
				<div class="row">
				<div class="col-sm-4">
					<div class="form-group">
					    <label for="lkCustomer.lkCustId" class="col-sm-3 control-label">Lk Cust Id:</label>
					    <div class="col-sm-9">
				   		<input type="text" class="form-control" id="lkCustomer.lkCustId" ng-model="model.lkCustomer.lkCustId" />
					   	</div>
					</div>
				</div>
				</div>
				<div class="row">
				<div class="col-sm-4">
					<div class="form-group">
					    <label for="lkCustomer.lkCustName" class="col-sm-3 control-label">Lk Cust Name:</label>
					    <div class="col-sm-9">
				   		<input type="text" class="form-control" id="lkCustomer.lkCustName" ng-model="model.lkCustomer.lkCustName" />
					   	</div>
					</div>
				</div>
				</div>
				<div class="row">
				<div class="col-sm-4">
					<div class="form-group">
					    <label for="lkCustomer.lkCustAddress" class="col-sm-3 control-label">Lk Cust Address:</label>
					    <div class="col-sm-9">
				   		<input type="text" class="form-control" id="lkCustomer.lkCustAddress" ng-model="model.lkCustomer.lkCustAddress" />
					   	</div>
					</div>
				</div>
				</div>
				<div class="row">
				<div class="col-sm-4">
					<div class="form-group">
					    <label for="lkCustomer.lkCustPhone" class="col-sm-3 control-label">Lk Cust Phone:</label>
					    <div class="col-sm-9">
				   		<input type="text" class="form-control" id="lkCustomer.lkCustPhone" ng-model="model.lkCustomer.lkCustPhone" />
					   	</div>
					</div>
				</div>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="panel panel-default">
			<div class="panel-heading"></div>
			<div class="panel-body">
				<div class="row">
					<table class="table">
						<thead>
							<tr>
								<th>Lk Card Ind</th>
								<th>Lk Card Number</th>
								<th>Lk Card Prefix</th>
								<th>Lk Card Sug Sherut</th>
							</tr>
						</thead>
						<tr data-ng-repeat="lkCustomer_LkCards in model.lkCustomer.LkCards">
							<td>{{lkCustomer_LkCards.lkCardInd}}</td>
							<td>{{lkCustomer_LkCards.lkCardNumber}}</td>
							<td>{{lkCustomer_LkCards.lkCardPrefix}}</td>
							<td>{{lkCustomer_LkCards.lkCardSugSherut}}</td>
						</tr>
			  		</table>
		  		</div>		
	  		</div>		
			</div>
		</div>
	</div>
</form>
