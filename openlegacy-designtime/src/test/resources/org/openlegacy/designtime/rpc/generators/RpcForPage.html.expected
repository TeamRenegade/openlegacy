		 <!--/sidebar -->
		 
		<div class="col-sm-9 col-md-10"><!--  main   -->
		
			<ol class="breadcrumb hidden-xs">
							<li><span class="active">XxolakkrParms</span></li>
			</ol>
			
			
				<h1>Xxolakkr Parms</h1>
			
			<div class="panel panel-default">
				<div class="panel-body">
	<nav class="navbar navbar-default hidden-xs" role="navigation">
	    <form class="navbar-form xnavbar-left" role="navigation">
					<button data-ng-click="doAction('XxolakkrParms', 'update')" class="btn btn-primary navbar-btn">Update</button>
			<a ng-href="{{baseUrl}}XxolakkrParms{{'/excel'}}" class="btn btn-primary navbar-btn pull-right" ng-show="true">	        	
            	Excel
            </a>            
	    </form>
	</nav>    
						<div class="panel-heading"><h3>Lk Customer</h3></div>		
<div class="panel panel-default">
<div class="panel-body">
		<form role="form">
			<div class="row">
				<div class="col-sm-6">
	<div class="form-group">		
		<label for="lkCustomer.lkCustInd" class="control-label">Lk Cust Ind:</label>		
   				<div id="lkCustomer.lkCustInd" ng-model="model.lkCustInd">{{model.lkCustInd}}</div>
	</div>
	<div class="form-group">		
		<label for="lkCustomer.lkCustId" class="control-label">Lk Cust Id:</label>		
				  	<input type="text" class="form-control" id="lkCustomer.lkCustId" ng-model="model.lkCustId">
	</div>
	<div class="form-group">		
		<label for="lkCustomer.lkCustName" class="control-label">Lk Cust Name:</label>		
   				<div id="lkCustomer.lkCustName" ng-model="model.lkCustName">{{model.lkCustName}}</div>
	</div>
	<div class="form-group">		
		<label for="lkCustomer.lkCustAddress" class="control-label">Lk Cust Address:</label>		
   				<div id="lkCustomer.lkCustAddress" ng-model="model.lkCustAddress">{{model.lkCustAddress}}</div>
	</div>
	<div class="form-group">		
		<label for="lkCustomer.lkCustPhone" class="control-label">Lk Cust Phone:</label>		
   				<div id="lkCustomer.lkCustPhone" ng-model="model.lkCustPhone">{{model.lkCustPhone}}</div>
	</div>
				</div>				
			</div>
		</form>		
</div>
</div>	
								
<div class="panel panel-default">
<div class="panel-body">
		<div class="table-responsive">
			<table class="table">
				<thead>
					<tr>
								<th>Lk Card Ind</th>
								<th>Lk Card Number</th>
								<th>Lk Card Prefix</th>
								<th class="hidden-xs">Lk Card Sug Sherut</th>
					</tr>
				</thead>
				<tbody>				
                    <tr data-ng-repeat="lkCustomer.LkCards in model.lkCustomer.LkCards">
							<td>{{lkCustomer.LkCards.lkCardInd}}</td>
							<td>{{lkCustomer.LkCards.lkCardNumber}}</td>
							<td>{{lkCustomer.LkCards.lkCardPrefix}}</td>
							<td>{{lkCustomer.LkCards.lkCardSugSherut}}</td>
					</tr>                            
                </tbody>
	  		</table>
  		</div>
  		
  		 <div class="top-margin-50">
            <button ng-click="doAction('XxolakkrParms', 'next')" type="button" class="btn btn-default navbar-btn pull-right">
                Next
                <span class="glyphicon glyphicon-forward"></span>
            </button>
            <button ng-click="doAction('XxolakkrParms', 'previous')" type="button" class="btn btn-default navbar-btn pull-left">
                <span class="glyphicon glyphicon-backward"></span>
                Prev
            </button>
        </div>                
</div>
</div>	
				</div>
			</div>
		</div><!--  /main   -->
	</div>
</div>									
										

