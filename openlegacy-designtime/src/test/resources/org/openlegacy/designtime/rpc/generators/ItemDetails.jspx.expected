<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:ol="urn:jsptagdir:/WEB-INF/tags/web" 
	xmlns:c="http://java.sun.com/jsp/jstl/core" 
	xmlns:fn="http://java.sun.com/jsp/jstl/functions"
	xmlns:spring="http://www.springframework.org/tags"
	xmlns:form="http://www.springframework.org/tags/form"
	version="2.0">
	<jsp:directive.page contentType="text/html;charset=UTF-8" pageEncoding="UTF-8" /> 
	<jsp:output omit-xml-declaration="yes" />
	
	<ol:form model="itemDetails">
		<ol:pagePart title="ItemDetails">
	<ol:row columns="1">
		<ol:field name="itemNum" />
	</ol:row>
		</ol:pagePart>
		<ol:pagePart title="ItemRecord">
	<ol:row columns="1">
		<ol:field name="itemRecord.itemName" />
	</ol:row>
	<ol:row columns="1">
		<ol:field name="itemRecord.description" />
	</ol:row>
	<ol:row columns="1">
		<ol:field name="itemRecord.weight" />
	</ol:row>
		</ol:pagePart>
		<ol:pagePart title="Shipping">
	<ol:row columns="1">
		<ol:field name="shipping.shippingMethod" />
	</ol:row>
	<ol:row columns="1">
		<ol:field name="shipping.days" />
	</ol:row>
		</ol:pagePart>
	</ol:form>
</jsp:root>
