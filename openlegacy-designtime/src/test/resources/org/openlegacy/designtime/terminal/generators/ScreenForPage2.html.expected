			<div class="table span12">
				<table>
					<thead>
						<tr>
							<th>Action</th>
							<th>Column1</th>
							<th>Column2</th>
						</tr>
					</thead>
					<tr data-ng-repeat="screenForPageRow in model.screenForPageRows">
						<td>{{screenForPageRow.action}}</td>
						<td>{{screenForPageRow.column1}}</td>
						<td>{{screenForPageRow.column2}}</td>
					</tr>
		  		</table>
	  		</div>		
			<button data-ng-click="help()" class="btn btn-small btn-primary">Help</button>
			<button data-ng-click="exit()" class="btn btn-small btn-primary">Exit</button>
