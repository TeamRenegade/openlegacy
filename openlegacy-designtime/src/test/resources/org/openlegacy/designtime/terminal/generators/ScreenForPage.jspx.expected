<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:ol="urn:jsptagdir:/WEB-INF/tags/web" 
	xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:fn="http://java.sun.com/jsp/jstl/functions"
	xmlns:spring="http://www.springframework.org/tags"
	xmlns:form="http://www.springframework.org/tags/form"
	version="2.0">
	<jsp:directive.page contentType="text/html;charset=UTF-8" pageEncoding="UTF-8" /> 
	<jsp:output omit-xml-declaration="yes" /> 

	<ol:form model="screenForPage">
			<ol:pagePart>
				<ol:row columns="1">
	 				<ol:field name="fldCol12" />
				</ol:row>
				<ol:row columns="1">
	 				<ol:field name="fld2Col12" />
				</ol:row>
				<ol:row columns="1">
	 				<ol:field name="fld3Col12" />
				</ol:row>
			</ol:pagePart>
			<ol:pagePart>
				<ol:row columns="2">
	 				<ol:field name="fldRow10" />
	 				<ol:field name="fld2Row10" />
				</ol:row>
			</ol:pagePart>
			<ol:pagePart>
				<ol:field name="orphanFld" />
			</ol:pagePart>
			<ol:pagePart>
				<ol:row columns="2">
	 				<ol:field name="screenForPagePart.fldRow20" />
	 				<ol:field name="screenForPagePart.fld2Row20" />
				</ol:row>
			</ol:pagePart>
			<ol:pagePart>
					<ol:table data="${screenForPage.screenForPageRows}" property="screenForPageRows"  />
			</ol:pagePart>
		
	</ol:form>
</jsp:root>
