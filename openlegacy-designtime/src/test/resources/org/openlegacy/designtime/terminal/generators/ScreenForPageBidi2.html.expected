	<h1>Screen For Page</h1>


	<nav class="navbar navbar-default" role="navigation">
	    <form class="navbar-form xnavbar-left" role="navigation">
	    	<button data-ng-click="doAction('ScreenForPage', '')" class="btn btn-primary navbar-btn">Submit</button>
				<button data-ng-show="isActionAvailable('help')" data-ng-click="doAction('ScreenForPage', 'help')" class="btn btn-primary navbar-btn">Help</button>
				<button data-ng-show="isActionAvailable('exit')" data-ng-click="doAction('ScreenForPage', 'exit')" class="btn btn-primary navbar-btn">Exit</button>
			<a ng-href="{{baseUrl}}ScreenForPage{{'/excel'}}" class="btn btn-primary navbar-btn pull-right" ng-show="true">
			<span class="glyphicon glyphicon-export"></span>	        	
            	Excel
            </a>            
	    </form>
	</nav>    
	<div class="panel panel-default">
	  	<div class="panel-heading" data-toggle="collapse" data-target="#screenforpagepart1">
	  		<h4 class="panel-title">
	  			
			</h4>
		</div>                	
	  	<div id="screenforpagepart1" class="panel-collapse collapse in">
			<div class="panel-body">
	          				
					<form role="form" class="form-horizontal">			
	<div class="form-group" ng-class="terminalColors('orphanFld')">
		<label for="orphanFld" class="control-label col-sm-2">Orphan Fld:</label>
		<div class="col-sm-6">
					<span id="orphanFld" ng-model="model.orphanFld">{{model.orphanFld}}</span>
   		</div>					
	</div>
					</form>
			</div>
		</div>
	</div>		
	<div class="panel panel-default">
	  	<div class="panel-heading" data-toggle="collapse" data-target="#screenforpagepart2">
	  		<h4 class="panel-title">
	  			
			</h4>
		</div>                	
	  	<div id="screenforpagepart2" class="panel-collapse collapse in">
			<div class="panel-body">
	          				
					<form role="form" class="form-horizontal">			
	<div class="form-group" ng-class="terminalColors('fldCol12')">
		<label for="fldCol12" class="control-label col-sm-2">Fld Col12:</label>
		<div class="col-sm-6">
		   			<input data-ng-class="readOnlyCss(model,'fldCol12')" data-ng-readonly="isReadOnly(model,'fldCol12')" type="text" class="form-control child-terminal-colors" id="fldCol12" ng-model="model.fldCol12">	   			   			
   		</div>					
	</div>
	<div class="form-group" ng-class="terminalColors('fld2Col12')">
		<label for="fld2Col12" class="control-label col-sm-2">Fld2Col12:</label>
		<div class="col-sm-6">
		   			<input data-ng-class="readOnlyCss(model,'fld2Col12')" data-ng-readonly="isReadOnly(model,'fld2Col12')" type="text" class="form-control child-terminal-colors" id="fld2Col12" ng-model="model.fld2Col12">	   			   			
   		</div>					
	</div>
	<div class="form-group" ng-class="terminalColors('fld3Col12')">
		<label for="fld3Col12" class="control-label col-sm-2">Fld3Col12:</label>
		<div class="col-sm-6">
		   			<input data-ng-class="readOnlyCss(model,'fld3Col12')" data-ng-readonly="isReadOnly(model,'fld3Col12')" type="text" class="form-control child-terminal-colors" id="fld3Col12" ng-model="model.fld3Col12">	   			   			
   		</div>					
	</div>
					</form>
			</div>
		</div>
	</div>		
	<div class="panel panel-default">
	  	<div class="panel-heading" data-toggle="collapse" data-target="#screenforpagepart3">
	  		<h4 class="panel-title">
	  			
			</h4>
		</div>                	
	  	<div id="screenforpagepart3" class="panel-collapse collapse in">
			<div class="panel-body">
	          				
					<form role="form" class="form-horizontal">			
	<div class="form-group" ng-class="terminalColors('fld2Row10')">
		<label for="fld2Row10" class="control-label col-sm-2">Fld2Row10:</label>
		<div class="col-sm-3">
					<span id="fld2Row10" ng-model="model.fld2Row10">{{model.fld2Row10}}</span>
   		</div>					
	</div>
	<div class="form-group" ng-class="terminalColors('fldRow10')">
		<label for="fldRow10" class="control-label col-sm-2">Fld Row10:</label>
		<div class="col-sm-3">
					<span id="fldRow10" ng-model="model.fldRow10">{{model.fldRow10}}</span>
   		</div>					
	</div>
					</form>
			</div>
		</div>
	</div>		
	<div class="panel panel-default">
	  	<div class="panel-heading" data-toggle="collapse" data-target="#screenforpagepart4">
	  		<h4 class="panel-title">
	  			Screen For Page Part
			</h4>
		</div>                	
	  	<div id="screenforpagepart4" class="panel-collapse collapse in">
			<div class="panel-body">
	          				
					<form role="form" class="form-horizontal">			
	<div class="form-group" ng-class="terminalColors('screenForPagePart.fld2Row20')">
		<label for="screenForPagePart.fld2Row20" class="control-label col-sm-2">Fld2Row20:</label>
		<div class="col-sm-3">
					<span id="screenForPagePart.fld2Row20" ng-model="model.screenForPagePart.fld2Row20">{{model.screenForPagePart.fld2Row20}}</span>
   		</div>					
	</div>
	<div class="form-group" ng-class="terminalColors('screenForPagePart.fldRow20')">
		<label for="screenForPagePart.fldRow20" class="control-label col-sm-2">Fld Row20:</label>
		<div class="col-sm-3">
					<span id="screenForPagePart.fldRow20" ng-model="model.screenForPagePart.fldRow20">{{model.screenForPagePart.fldRow20}}</span>
   		</div>					
	</div>
					</form>
			</div>
		</div>
	</div>		
	<div class="panel panel-default">
	  	<div class="panel-heading" data-toggle="collapse" data-target="#screenforpagepart5">
	  		<h4 class="panel-title">
	  			
			</h4>
		</div>                	
	  	<div id="screenforpagepart5" class="panel-collapse collapse in">
			<div class="panel-body">
	          				
					<div class="table-responsive">
						<table class="table">
							<thead>
								<tr>
									<th></th>
										<th>Column1</th>
										<th>Column2</th>
								</tr>
							</thead>
							<tbody>                            	
			                    <tr data-ng-repeat="screenForPageRow in model.screenForPageRows">
											<td ng-class="terminalColors('action_', screenForPageRow)">
																<a href="" class="child-terminal-colors" ng-click="screenForPageRow.action='1';doActionNoTargetEntity()">
																	View
																</a>
																<a href="" class="child-terminal-colors" ng-click="screenForPageRow.action='2';doActionNoTargetEntity()">
																	Revise
																</a>
			                            	</td>
			                                                  
											<td ng-class="terminalColors('column1', screenForPageRow)">
												{{screenForPageRow.column1}}
											</td>
											<td ng-class="terminalColors('column2', screenForPageRow)">
												{{screenForPageRow.column2}}
											</td>
								</tr>                          
			                </tbody>
				  		</table>
			  		</div>
			  		
			  		 <div class="top-margin-50">
			            <button ng-click="doAction('ScreenForPage', 'next')" type="button" class="btn btn-default navbar-btn pull-right">
			                Next
			                <span class="glyphicon glyphicon-forward"></span>
			            </button>
			            <button ng-click="doAction('ScreenForPage', 'previous')" type="button" class="btn btn-default navbar-btn pull-left">
			                <span class="glyphicon glyphicon-backward"></span>
			                Prev
			            </button>
			        </div>                
			</div>
		</div>
	</div>		