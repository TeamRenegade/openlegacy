<div class="row">
	<div class="col-sm-4">
		<h1>Screen For Page</h1>
	</div>
	<div class="col-sm-8">
		<h1>
			<button data-ng-click="help()" class="btn btn-lg btn-primary">Help</button>
			<button data-ng-click="exit()" class="btn btn-lg btn-primary">Exit</button>
		</h1>
	</div>
</div>

<form  role="form" accept-charset="utf-8">
	<div class="row">
		<div class="panel panel-default">
			<div class="panel-heading"></div>
			<div class="panel-body">
				<div class="row">
				<div class="col-sm-4">
					<div class="form-group">
					    <label for="fldCol12" class="col-sm-3 control-label">Fld Col12:</label>
					    <div class="col-sm-9">
				   		<input type="text" class="form-control" id="fldCol12" ng-model="model.fldCol12" />
					   	</div>
					</div>
				</div>
				</div>
				<div class="row">
				<div class="col-sm-4">
					<div class="form-group">
					    <label for="fld2Col12" class="col-sm-3 control-label">Fld2Col12:</label>
					    <div class="col-sm-9">
				   		<input type="text" class="form-control" id="fld2Col12" ng-model="model.fld2Col12" />
					   	</div>
					</div>
				</div>
				</div>
				<div class="row">
				<div class="col-sm-4">
					<div class="form-group">
					    <label for="fld3Col12" class="col-sm-3 control-label">Fld3Col12:</label>
					    <div class="col-sm-9">
				   		<input type="text" class="form-control" id="fld3Col12" ng-model="model.fld3Col12" />
					   	</div>
					</div>
				</div>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="panel panel-default">
			<div class="panel-heading"></div>
			<div class="panel-body">
				<div class="row">
				<div class="col-sm-4">
					<div class="form-group">
					    <label for="fld2Row10" class="col-sm-3 control-label">Fld2Row10:</label>
					    <div class="col-sm-9">
				   			<div id="fld2Row10" ng-model="model.fld2Row10"></div>
					   	</div>
					</div>
				</div>
				<div class="col-sm-4">
					<div class="form-group">
					    <label for="fldRow10" class="col-sm-3 control-label">Fld Row10:</label>
					    <div class="col-sm-9">
				   			<div id="fldRow10" ng-model="model.fldRow10"></div>
					   	</div>
					</div>
				</div>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="panel panel-default">
			<div class="panel-heading"></div>
			<div class="panel-body">
				<div class="row">
				<div class="col-sm-4">
					<div class="form-group">
					    <label for="orphanFld" class="col-sm-3 control-label">Orphan Fld:</label>
					    <div class="col-sm-9">
				   			<div id="orphanFld" ng-model="model.orphanFld"></div>
					   	</div>
					</div>
				</div>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="panel panel-default">
			<div class="panel-heading">Screen For Page Part</div>
			<div class="panel-body">
				<div class="row">
				<div class="col-sm-4">
					<div class="form-group">
					    <label for="screenForPagePart.fld2Row20" class="col-sm-3 control-label">Fld2Row20:</label>
					    <div class="col-sm-9">
				   			<div id="screenForPagePart.fld2Row20" ng-model="model.screenForPagePart.fld2Row20"></div>
					   	</div>
					</div>
				</div>
				<div class="col-sm-4">
					<div class="form-group">
					    <label for="screenForPagePart.fldRow20" class="col-sm-3 control-label">Fld Row20:</label>
					    <div class="col-sm-9">
				   			<div id="screenForPagePart.fldRow20" ng-model="model.screenForPagePart.fldRow20"></div>
					   	</div>
					</div>
				</div>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="panel panel-default">
			<div class="panel-heading"></div>
			<div class="panel-body">
				<div class="row">
					<table class="table">
						<thead>
							<tr>
								<th>Action</th>
								<th>Column1</th>
								<th>Column2</th>
							</tr>
						</thead>
						<tr data-ng-repeat="screenForPageRow in model.screenForPageRows">
							<td>{{screenForPageRow.action}}</td>
							<td>{{screenForPageRow.column1}}</td>
							<td>{{screenForPageRow.column2}}</td>
						</tr>
			  		</table>
		  		</div>		
	  		</div>		
			</div>
		</div>
	</div>
</form>
