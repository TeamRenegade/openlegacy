package com.test;

import org.openlegacy.annotations.screen.ScreenField;
import org.openlegacy.annotations.screen.Identifier;
import org.openlegacy.annotations.screen.ScreenEntity;
import org.openlegacy.annotations.screen.ScreenIdentifiers;

@ScreenEntity()
@ScreenIdentifiers(identifiers = { 
				@Identifier(row = 1, column = 40, value = "Screen 1"), 
				@Identifier(row = 4, column = 3, value = "Field A:"), 
				@Identifier(row = 5, column = 3, value = "Field B:") 
				})
public class Screen1 {
    
	@ScreenField(row = 4, column = 13 ,editable= true ,displayName = "Field A", sampleValue = "AAAA")
    private String fieldA;

	@ScreenField(row = 5, column = 13 ,endColumn= 20 ,displayName = "Field B", sampleValue = "BBBB")
    private String fieldB;

    


 
}
