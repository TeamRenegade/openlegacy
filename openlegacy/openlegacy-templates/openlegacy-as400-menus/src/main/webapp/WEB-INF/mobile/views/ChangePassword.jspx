<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:ol="urn:jsptagdir:/WEB-INF/tags/web" version="2.0">
	<jsp:directive.page contentType="text/html;charset=UTF-8"
		pageEncoding="UTF-8" />
	<jsp:output omit-xml-declaration="yes" />

	<ol:form model="changePassword">
		<ol:field name="userProfile" />
		<ol:field name="passwordLastChanged" />
		<ol:field name="currentPassword" />
		<ol:field name="newPassword" />
		<ol:field name="newPasswordtoVerify" />

	</ol:form>
</jsp:root>
