    @RequestMapping(value="/help", method = RequestMethod.GET)
    public @ResponseBody String WorkWithSubmittedJobsController.systemHelp(HttpServletRequest request) throws IOException {
    	URL resource = request.getSession().getServletContext().getResource("/help/WorkWithSubmittedJobs.html");
    	String result = "";
    	if (resource != null){
    		result = IOUtils.toString(resource.openStream());
    	}
    	return result;
    }

	// handle submit action
    @RequestMapping(method = RequestMethod.POST)
    public String WorkWithSubmittedJobsController.submit(WorkWithSubmittedJobs workWithSubmittedJobs, Model uiModel, HttpServletRequest httpServletRequest) {
	ScreenEntity resultScreenEntity = terminalSession.doAction(TerminalActions.ENTER(), workWithSubmittedJobs);
	// go to the controller for the resulting screen name
		if (resultScreenEntity != null){
			String screenEntityName = screenEntitiesRegistry.get(resultScreenEntity.getClass()).getEntityClassName();
			if (httpServletRequest.getParameter("partial") != null){
				workWithSubmittedJobs = terminalSession.getEntity(WorkWithSubmittedJobs.class);
		    	uiModel.addAttribute("workWithSubmittedJobs", workWithSubmittedJobs);
		        return "WorkWithSubmittedJobs";
			}
        	return "redirect:" + screenEntityName;
		}
    	return "redirect:/";
		
    }
    
