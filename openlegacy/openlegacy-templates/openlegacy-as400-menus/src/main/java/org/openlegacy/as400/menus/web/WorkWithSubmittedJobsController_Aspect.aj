	// handle page initial display
    @RequestMapping(method = RequestMethod.GET)
    public String WorkWithSubmittedJobsController.show(Model uiModel) {
    	WorkWithSubmittedJobs workWithSubmittedJobs = terminalSession.getEntity(WorkWithSubmittedJobs.class);
