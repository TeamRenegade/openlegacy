	// export to excel
    @RequestMapping(value="/excel", method = RequestMethod.GET)
    public void EditLibraryListController.excel(HttpServletResponse response) throws IOException {
    	List<EditLibraryList.EditLibraryListRecord> records = terminalSession.getModule(Table.class).collectOne(EditLibraryList.class,EditLibraryList.EditLibraryListRecord.class);
		response.setContentType("application/vnd.ms-excel");
		response.addHeader("Content-Disposition", "attachment; filename=\"EditLibraryList.xls\"");
    	tableWriter.writeTable(records, response.getOutputStream());
    }
	
	
	
	@InitBinder
