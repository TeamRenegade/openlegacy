	// handle page initial display
    @RequestMapping(method = RequestMethod.GET)
    public String EditLibraryListController.show(Model uiModel) {
    	EditLibraryList editLibraryList = terminalSession.getEntity(EditLibraryList.class);
	uiModel.addAttribute("editLibraryList", editLibraryList);
	// show the resulting page for EditLibraryList
        return "EditLibraryList";
    }

	
    @RequestMapping(value="/help", method = RequestMethod.GET)
    public @ResponseBody String EditLibraryListController.systemHelp(HttpServletRequest request) throws IOException {
    	URL resource = request.getSession().getServletContext().getResource("/help/EditLibraryList.html");
    	String result = "";
    	if (resource != null){
    		result = IOUtils.toString(resource.openStream());
    	}
    	return result;
    }

	// handle submit action
    @RequestMapping(method = RequestMethod.POST)
    public String EditLibraryListController.submit(EditLibraryList editLibraryList, Model uiModel, HttpServletRequest httpServletRequest) {
	ScreenEntity resultScreenEntity = terminalSession.doAction(TerminalActions.ENTER(), editLibraryList);
	// go to the controller for the resulting screen name
		if (resultScreenEntity != null){
			String screenEntityName = screenEntitiesRegistry.get(resultScreenEntity.getClass()).getEntityClassName();
			if (httpServletRequest.getParameter("partial") != null){
				editLibraryList = terminalSession.getEntity(EditLibraryList.class);
		    	uiModel.addAttribute("editLibraryList", editLibraryList);
		        return "EditLibraryList";
			}
        	return "redirect:" + screenEntityName;
		}
    	return "redirect:/";
		
    }
    
