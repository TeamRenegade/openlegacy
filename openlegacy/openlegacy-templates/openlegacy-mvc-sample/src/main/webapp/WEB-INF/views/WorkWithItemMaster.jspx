<div xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:ol="urn:jsptagdir:/WEB-INF/tags">
	<ol:form model="workWithItemMaster">
		<div class="table">
			<table>
				<tr>
					<th>Action</th>
					<th>Alpha search</th>
					<th>Item Description</th>
					<th>Item Number</th>
				</tr>
				<c:forEach items="${workWithItemMaster.workWithItemMasterRecords}"
					var="item" varStatus="i">
					<tr class="${i.index % 2 == 0 ? '' : 'alt'}">
						<td><a href="WorkWithItemMaster1/revise/${item.itemNumber}"
								class="rbtn revise" title="Revise">Revise</a> <a
								href="WorkWithItemMaster1/display/${item.itemNumber}"
								class="rbtn display" title="Display">Display</a> <a
								href="WorkWithItemMaster1/delete/${item.itemNumber}"
								class="rbtn delete" title="Delete">Delete</a></td>
						<td>${item.alphaSearch}</td>
						<td>${item.itemDescription}</td>
						<td>${item.itemNumber}</td>
					</tr>
				</c:forEach>
			</table>
		</div>
		<div class="paging">
			<div class="previous">
				<a
					href="javascript:doAjaxPost('WorkWithItemMasterForm','container','previous','body')">
					<img src="icons/previous.png" width="13" height="13"
						alt="Previous" /> <span>Previous</span>
				</a>
			</div>
			<div class="next">
				<a
					href="javascript:doAjaxPost('WorkWithItemMasterForm','container','next','body')">
					<span>Next</span> <img src="icons/next.png" width="13"
						height="13" alt="Next" />
				</a>
			</div>
		</div>
	</ol:form>
</div>
