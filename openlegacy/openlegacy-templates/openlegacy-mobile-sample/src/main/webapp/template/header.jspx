<h1 xmlns:c="http://java.sun.com/jsp/jstl/core"
	data-dojo-type="dojox.mobile.Heading" moveTo="${definitions.navigationDefinition.accessedFrom.simpleName}Panel"
	back="${definitions != null ? 'Back' : ''}" fixed="top">
	<img src="images/logo.png" class="logo" />
	<c:if test="${loggedInUser != null}">
		<button data-dojo-type="dojox.mobile.ToolBarButton" fixed="right"
			class="logoffBtn"
			onclick="location.href='${pageContext.request.contextPath}/logoff'">Logoff</button>
	</c:if>
</h1>
