<html xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:tiles="http://tiles.apache.org/tags-tiles"
	xmlns:fn="http://java.sun.com/jsp/jstl/functions"
	xmlns:spring="http://www.springframework.org/tags">

	<jsp:directive.page contentType="text/html;charset=UTF-8" />
	<jsp:directive.page pageEncoding="UTF-8" />
	<head>
		<meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1,minimum-scale=1,user-scalable=no"/>
		<meta name="apple-mobile-web-app-capable" content="yes" />
		<link rel="shortcut icon" href="${pageContext.request.contextPath}/favicon.ico" />
		<title>OpenLegacy Mobile Demo</title>
		
		<script type="text/javascript">var dojoConfig = {parseOnLoad: true,preventBackButtonFix: false, baseUrl: '${pageContext.request.contextPath}/dj/dojo/', mblAlwaysHideAddressBar: true};</script>
		
		<script src="${pageContext.request.contextPath}/dj/dojo.mobile.custom.build.js" type="text/javascript"><!-- aa --></script>
		
		<script src="${pageContext.request.contextPath}/openlegacy-js/openlegacy.mvc.js" type="text/javascript"><!-- required for FF3 and Opera --></script>
		<script src="${pageContext.request.contextPath}/js/demo.js" type="text/javascript"><!-- required for FF3 and Opera --></script>

		<link href="${pageContext.request.contextPath}/themes/cloudm/cloudm.css" rel="stylesheet"></link>
		<link href="${pageContext.request.contextPath}/dj/dijit.css" rel="stylesheet" />		
		<link href="${pageContext.request.contextPath}/themes/cloudm/openlegacy.css" rel="stylesheet" />
		<script language="JavaScript" type="text/javascript">
			dojo.require("dojox.mobile.parser");
			dojo.require("dojox.mobile");
			//dojo.require("dojox.mobile.deviceTheme");
			dojo.requireIf(!dojo.isWebKit, "dojox.mobile.compat");
			dojo.require("dojox.mobile.ScrollableView");
			dojo.require("dojox.mobile.TextBox");
			dojo.require("dojox.mobile.Button");
			dojo.require("dojox.mobile.RoundRectList");
			dojo.require("dojox.mobile.ListItem");
			dojo.require("dojox.mobile.RoundRectCategory");
			dojo.require("dojox.mobile.ScrollableView");
			dojo.require("dojox.mobile.ToolBarButton");
			dojo.require("dojox.mobile.ContentPane");
			dojo.require("dojox.mobile.View");
			dojo.require("dojox.mobile.SwapView");
			dojo.require("dojox.mobile.RoundRect");
			dojo.require("dojox.mobile.Heading");
			dojo.require("dojox.mobile.TextArea");
			dojo.require("dojox.mobile.CheckBox");
			dojo.require("dojox.mobile.TabBar");
			dojo.require("dojox.mobile.TabBarButton");
			dojo.require("dojo.data.ItemFileReadStore");
			dojo.require("dojox.mobile.ComboBox");
			dojo.require("dojox.mobile.Opener");
			dojo.require("dojo.back");
			dojo.require("dijit.Calendar");
			dojo.require("dojo.store.Memory");
			dojo.require("dojox.mobile.ComboBox");
			dojo.back.init();
			dojo.back.setInitialState(new HistoryState("DemoEnvironmentPanel"));			
		</script>
	</head>
	<body>

		<div id="container" class="container" data-dojo-type="dojox.mobile.ContentPane">
			<tiles:insertAttribute name="header" ignore="true"/>
			<tiles:insertAttribute name="menu" ignore="true"/>
			<tiles:insertAttribute name="body" />
		</div>
		
		<div id="loadingMessage" style="position: absolute;top: 300px;left: 200px;display: none; ">
			<img src="${pageContext.request.contextPath}/icons/loading.gif" />
		</div>		

		<tiles:insertAttribute name="footer" ignore="true"/>
	</body>
</html>
