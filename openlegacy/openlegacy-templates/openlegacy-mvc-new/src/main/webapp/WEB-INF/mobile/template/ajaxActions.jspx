<h1 xmlns:c="http://java.sun.com/jsp/jstl/core" data-dojo-type="dojox.mobile.Heading" fixed="bottom">

	<button data-dojo-type="dojox.mobile.Button" class="mblBtn baseBtn" onclick="doMobilePost('${definitions.entityName}Form','${definitions.entityName}Panel');">Submit</button>
	<c:forEach items="${definitions.actions}" var="action">
		<button data-dojo-type="dojox.mobile.Button" class="mblBtn baseBtn" onclick="doMobilePost('${definitions.entityName}Form','${definitions.entityName}Panel','${action.alias}');" >${action.displayName}</button> 
	</c:forEach>
</h1>
